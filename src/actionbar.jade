footer.actionbar
  a.media(href="javascript:void(0);",ng-click="clickUpload();" )
  input(id="upload", type="file",name="file", ng-file-select="fileChange($files)",accept="image/*",capture, ng-show="false")
  span.box
    input.text(type="text", ng-model="message", ng-enter, ng-focus="showEnter=true")
  a.enter(ng-click="say();", ng-show="showEnter && message.length > 0")
